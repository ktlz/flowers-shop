.checkout
    display flex
    flex-direction column
    align-items center
    margin-top 60px
    .empty-title
        font-size 24px
        line-height 18px
        letter-spacing: 0.2px
        color #CA6791
        text-align center
        margin 15px
        margin-bottom 25px

    .cart-image
        display flex
        background-image url('../../../assets/img/cart.svg')
        width 55px
        height 50px
        background-size contain
        background-repeat no-repeat
        margin-bottom 36px
    .catalog
        border-bottom 1px solid #81AA94
        text-decoration none
        color #1D1C1C
        padding-bottom 10px